{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build",
                "@workspace/api-client#generate"
            ],
            "outputs": [
                ".next/**",
                "dist/**"
            ]
        },
        "generate": {
            "cache": false
        },
        "dev": {
            "cache": false
        }
    }
}